# LeadFactory Environment Variables Example

# Environment
ENVIRONMENT=development
DEBUG=false
SECRET_KEY=your-secret-key-here

# Database
DATABASE_URL=postgresql://leadfactory:leadfactory123@postgres:5432/leadfactory
POSTGRES_USER=leadfactory
POSTGRES_PASSWORD=leadfactory123
POSTGRES_DB=leadfactory

# Redis
REDIS_URL=redis://redis:6379/0

# API Configuration
USE_STUBS=true
STUB_BASE_URL=http://stub-server:5010

# External API Keys
YELP_API_KEY=your-yelp-api-key-here
GOOGLE_API_KEY=your-google-api-key-here
GOOGLE_PAGESPEED_API_KEY=your-pagespeed-api-key-here
STRIPE_SECRET_KEY=your-stripe-secret-key-here
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key-here
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret-here
STRIPE_PRICE_ID=price_1234567890
SENDGRID_API_KEY=your-sendgrid-api-key-here
OPENAI_API_KEY=your-openai-api-key-here

# Phase 0.5 API Keys
DATA_AXLE_API_KEY=your-data-axle-api-key-here
HUNTER_API_KEY=your-hunter-api-key-here

# Email Settings
FROM_EMAIL=noreply@leadfactory.com
FROM_NAME=LeadFactory

# Cost Control
COST_BUDGET_USD=1000.0

# Feature Flags
ENABLE_ENRICHMENT=true
ENABLE_LLM_INSIGHTS=true
ENABLE_EMAIL_TRACKING=true
ENABLE_EXPERIMENTS=false
PROVIDERS_DATA_AXLE_ENABLED=true
PROVIDERS_HUNTER_ENABLED=false

# Performance
MAX_CONCURRENT_ASSESSMENTS=10
REQUEST_TIMEOUT=30

# Monitoring
PROMETHEUS_ENABLED=true

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Observability
DATADOG_API_KEY=<PUT-YOUR-KEY-HERE>
LF_ENV=local

# Error tracking
SENTRY_DSN=<PUT-YOUR-DSN-HERE>
SENTRY_TRACE_RATE=0.20